{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "Node",
        "target": "ESNext",
        "lib": ["ESNext"],
        "outDir": "dist",
        "rootDir": "src",
        "types": ["node"],
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "typeRoots": [
            "node_modules/@types",
            "node_modules/discord.js/typings"
        ],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules",
        "dev"
    ],
    "include": [
        "src"
    ]
}